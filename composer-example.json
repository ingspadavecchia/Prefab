{
  "name": "neighborhoods/PROJECTNAME",
  "description": "Add a short description of your project here.",
  "type": "project",
  "license": "proprietary",
  "keywords": [
    "prefab"
  ],
  "scripts": {
    "post-update-cmd": [
      "vendor/bin/prefab",
      "@composer dump-autoload -a",
      "vendor/bin/bakery"
    ],
    "post-install-cmd": [
      "vendor/bin/prefab",
      "@composer dump-autoload -a",
      "vendor/bin/bakery"
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:neighborhoods/prefab.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:neighborhoods/Bakery.git"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.2",
    "neighborhoods/prefab": "4.x-dev",
    "neighborhoods/bakery": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Neighborhoods\\PROJECTNAME\\": [
        "src",
        "fab"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neighborhoods\\PROJECTNAME\\": [
        "src/",
        "test-fab/"
      ]
    }
  }
}
