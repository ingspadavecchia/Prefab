{
  "name": "neighborhoods/prefab-test-app",
  "description": "A dummy implementation of a Prefab test app for acceptance testing.",
  "type": "project",
  "license": "proprietary",
  "keywords": [
    "prefab"
  ],
  "minimum-stability": "dev",
  "repositories": {
    "Prefab": {
      "type": "artifact",
      "url": "prefab"
    }
  },
  "scripts": {
    "archive-package": [
      "mkdir -p prefab-archive",
      "@composer archive --working-dir=../../ --ignore-filters --dir=tests/test-app/prefab-archive --file=my-package"
    ],
    "pre-install-cmd": "@archive-package",
    "pre-update-cmd": "@archive-package"
  },
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^7.2",
    "neighborhoods/prefab": "@dev"
  },
  "autoload": {
    "psr-4": {
      "Neighborhoods\\PrefabTestApp\\": [
        "src",
        "fab"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neighborhoods\\PrefabTestApp\\": [
        "src/",
        "test-fab/"
      ]
    }
  }
}
